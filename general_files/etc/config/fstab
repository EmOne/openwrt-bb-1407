config 'global'
	option  anon_swap       '0'
	option  anon_mount      '0'
	option  auto_swap       '1'
	option  auto_mount      '1'
	option  delay_root      '5'
	option  check_fs        '0'

config global automount
	option from_fstab '1'
	option anon_mount '1'
	
config global autoswap
	option from_fstab '1'
	option anon_swap '0'
	
config 'mount'
	option 'device'		'/dev/sda1'
	option 'target'		'/mnt/sda1'
	option 'fstype'		'auto'

config 'mount'
	option 'device'		'/dev/sdb1'
	option 'target'		'/mnt/sdb1'
	option 'fstype'		'auto'

config 'mount'
	option 'device'		'/dev/sdc1'
	option 'target'		'/mnt/sdc1'
	option 'fstype'		'auto'

config swap
	option device	'/dev/sda2'
	option enabled	'0'

