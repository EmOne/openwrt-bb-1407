config system
	option timezone 'UTC'
	option hostname 'dragino2'
	option firstboot '1'

config timeserver 'ntp'
	list server '0.openwrt.pool.ntp.org'
	list server '1.openwrt.pool.ntp.org'
	list server '2.openwrt.pool.ntp.org'
	list server '3.openwrt.pool.ntp.org'
	option enable_server '0'

config restorefactory
	option button 'reset'
	option action 'pressed'
	option timeout '5'

config oem 'vendor'
	option name 'SuissePhone'
	option ssid_prefix 'SuissePhone'
	option hostname 'SuissePhone'
	option web_model 'SuissePhone'
	option company_name 'Globacom AG'
	option company_url 'http://www.globacom.com'